  <% if current_user %>
      <%= link_to "Log out", logout_path, method: :delete%><br><br>

  <% else %>
    <%= form_with url: login_path, method: :post, local: true do |f| %>
      <%= f.label :email, 'Email:' %>
      <%= f.text_field :email %><br><br>
      <%= f.label :password, 'Password:' %>
      <%= f.password_field :password %><br><br>
      <%= f.submit 'Log In' %><br><br>
    <% end %>

    <%= link_to "Register New Account", registration_path %><br><br>

  <% end %>